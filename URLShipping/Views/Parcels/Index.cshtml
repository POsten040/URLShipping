@using URLShipping.Models;

  <h1>Your shipping cost.</h1>
  @if (Model.Count == 0)
  {
    <p>There are no packages in your list.</p>
  }
  <ul>
    @foreach (Parcel parcel in Model)
    {
      <li>@parcel.Name: $@parcel.CostToShip()</li>
    }
  </ul>
  <a href="/parcels/new">Price out another package.</a>
  

  